{
   "name":"Clean Twitter",
   "short_name": "Clean Twitter",
   "version":"1.0.9",
   "description":"Remove clutter from twitter interface and make it lean and clean.",
   "homepage_url": "https://github.com/ostoh/clean-twitter",
   "manifest_version":2,
   "icons":{
      "128":"icons/128.png"
   },
   "content_scripts":[
      {
         "run_at": "document_start",
         "matches":[
         "https://twitter.com/*",
         "https://mobile.twitter.com/*"
      ],
       "css":[
         "override.css"
      ],
       "js":[
         "override.js",
         "background.js"
      ]
      }
   ]
}